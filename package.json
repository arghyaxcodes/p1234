{
  "name": "p1234",
  "type": "module",
  "version": "0.1.2",
  "description": "A Size Limited Font Collection for Serverless Environment. Including 1899 Google Fonts!",
  "keywords": [
    "tiny",
    "nano",
    "font",
    "collection",
    "serverless"
  ],
  "homepage": "https://github.com/arghyaxcodes/p1234#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arghyaxcodes/p1234.git"
  },
  "bugs": {
    "url": "https://github.com/arghyaxcodes/p1234/issues"
  },
  "main": "fonts/index.js",
  "types": "fonts/index.d.ts",
  "exports": {
    "types": "./fonts/index.d.ts",
    "import": "./fonts/index.js",
    "require": "./fonts/index.cjs",
    "default": "./fonts/index.js"
  },
  "files": [
    "fonts"
  ],
  "scripts": {
    "gen": "tsx ./src",
    "build": "npm run gen && tsup",
    "dev": "tsup --watch",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "clean": "rm -rf fonts generated"
  },
  "author": "Arghya Ghosh <hello@arghya.dev>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^25.0.3",
    "dotenv": "^17.2.3",
    "fonteditor-core": "^2.3.2",
    "node-fetch": "^3.3.2",
    "ora": "^9.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
